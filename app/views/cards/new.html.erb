<body id="home">
  <%= image_tag("logo.png", alt: "Logo") %>
  <div>
    <div class="card-input">
      <%= form_for @card do |f| %>
        </br></br></br></br>
        <%= f.select :card_name, options_from_collection_for_select(Psa_pop.select(:psa_card).distinct.order(:psa_card), :psa_card, :psa_card, ) %>
        </br></br></br>
        <%= f.submit "Submit"%>
      <% end %>
    </div>
  </div>
</body>
